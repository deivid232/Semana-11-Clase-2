<p id="notice"><%= notice %></p>
    <h1 style="background-color: blue;">Students</h1>
<table class="table table-hover">
  <thead>
    <th>name</th>
    <th>Email</th>
    <th colspan="3"></th>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr class="table-active">
        <td><%= student.name %></td>
        <td><%= student.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<%= link_to 'New Student', new_student_path %>
